{
    "name": "ryancco/laravel-uuid-models",
    "description": "Trait for working with Eloquent models with UUID route keys",
    "keywords": [
        "laravel",
        "uuid",
        "routing"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Colson",
            "email": "ryanccolson@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8",
        "illuminate/support": "^7|^8"
    },
    "autoload": {
        "psr-4": {
            "Ryancco\\HasUuidRouteKey\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ryancco\\HasUuidRouteKey\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6",
        "phpunit/phpunit": "^9"
    }
}
